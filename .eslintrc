{
    "extends": [
      "airbnb",
      "prettier"
    ],
    "env":{
      "browser": true
    },
    "plugins": [
      "prettier",
      "react",
      "jsx-a11y",
      "import"
    ],
    "rules": {
      "extensions": [".js", ".jsx"],
      "react/prop-types": 0,
      "no-underscore-dangle": 0,
      "import/imports-first": [
        "error",
        "absolute-first"
      ],
      "import/newline-after-import": "error"
    },
    "globals": {
      "window": true,
      "document": true,
      "localStorage": true,
      "FormData": true,
      "FileReader": true,
      "Blob": true,
      "navigator": true
    },
    "parser": "babel-eslint"
  }